{
  "name": "assasment-cced-unila-backend",
  "version": "0.8.0",
  "author": {
    "name": "Muhammad Azka Naufal",
    "email": "mhmdazkanfl@gmail.com"
  },
  "scripts": {
    "test": "bun test",
    "dev": "NODE_ENV=development bun --watch ./src/index.ts",
    "build": "NODE_ENV=production bun build ./src/index.ts --target bun --outdir ./build",
    "start": "NODE_ENV=production bun ./build/index.js",
    "db:push": "bun --bun drizzle-kit push",
    "db:generate": "bun --bun drizzle-kit generate",
    "db:migrate": "bun --bun drizzle-kit migrate"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.11",
    "@faker-js/faker": "^10.1.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "drizzle-typebox": "^0.3.3",
    "elysia": "1.4.13",
    "picocolors": "^1.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "bun-types": "1.3.1",
    "drizzle-kit": "^0.31.5"
  },
  "module": "src/index.js",
  "overrides": {
    "@sinclair/typebox": "0.34.8"
  }
}
