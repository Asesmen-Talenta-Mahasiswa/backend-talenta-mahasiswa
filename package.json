  },
  "scripts": {
    "test": "bun test",
    "dev": "bun --watch ./src/index.ts",
    "build": "bun build ./src/index.ts --target bun --outdir ./build",
    "start": "NODE_ENV=production bun ./build/index.js",
    "db:push": "bun --bun drizzle-kit push",
    "db:generate": "bun --bun drizzle-kit generate",
    "db:migrate": "bun --bun drizzle-kit migrate"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.3",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "elysia": "1.4.6",
    "postgres": "^3.4.7",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "bun-types": "1.2.22",
    "drizzle-kit": "^0.31.4"
  },
  "module": "src/index.js"
}
